@model List<I_Car_Services.Models.Service>

<div class="container mt-5">
    <h3 class="text-center mb-3">Manage Your Services</h3>
    <table class="table table-bordered">
        <thead class="table-dark">
            <tr>
                <th>Service Name</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var service in Model)
            {
                <tr>
                    <td>@service.Name</td>
                    <td>@service.Price</td>
                </tr>
            }
        </tbody>
    </table>
</div>
